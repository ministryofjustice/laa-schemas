{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/ministryofjustice/laa-schemas/main/prototyping/general/schemas/person.json",
  "title": "Person",
  "description": "Holds attributes of a person",
  "type": "object",
  "properties": {
    "first_name": { "type": "string" },
    "last_name": { "type": "string" },
    "date_of_birth": { "type": "string", "format": "date" },
    "national_insurance_number": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "address": { "$ref": "https://raw.githubusercontent.com/ministryofjustice/laa-schemas/main/prototyping/general/schemas/address.json" }
  },
  "required": ["first_name", "last_name"]
}
