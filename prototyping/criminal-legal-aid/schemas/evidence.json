{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/ministryofjustice/laa-schemas/main/prototyping/criminal-legal-aid/schemas/evidence.json",
  "title": "Evidence",
  "description": "Holds attributes for items of evidence for criminal legal aid applications",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": { "type": "string" },
      "key": { "type": "string" },
      "description": { "type": "string" },
      "uploaded_date": { "type": "string", "format": "date" }
    }
  }
}
